<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="university" class="com.example.entity.University">
		<property name="sub1" value="Computer Architecture" />
		<property name="sub2" value="Operating Systen" />
	</bean>
	<bean id="college" class="com.example.entity.College" parent="university">
		<property name="sub2" value="Java" />
		<property name="sub3" value="Spring" />
		<!--	<property name="sub4" value="JSP" />-->
		<property name="sub5" value="Servlets" />
	</bean>

	<bean id="javaCollection" class="com.example.entity.JavaCollection">
		<!-- results in a setAddressList(java.util.List) call -->
		<property name="addresslist">
			<list>
				<value>INDIA</value>
				<value>Pakistan</value>
				<value>USA</value>
				<value>USA</value>
				<value>Canada</value>
				<value>INDIA</value>
				<value>China</value>
			</list>
		</property>
		<property name="addressSet">
			<set>
				<value>INDIA</value>
				<value>Pakistan</value>
				<value>USA</value>
				<value>USA</value>
				<value>Canada</value>
				<value>INDIA</value>
				<value>China</value>
			</set>
		</property>
		<property name="addressMap">
			<map>
				<entry key="1" value="INDIA" />
				<entry key="2" value="USA" />
				<entry key="3" value="China" />
				<entry key="4" value="Canada" />
				<entry key="5" value="INDIA" />
				<entry key="6" value="Canada" />
				<entry key="7" value="INDIA" />
				<entry key="8" value="Pakistan" />
				<entry key="9" value="USA" />
				<entry key="1" value="NEW country" />
				<entry key="22" value="USA" />
				<entry key="31" value="China" />
				<entry key="33" value="Canada" />
				<entry key="5" value="NEW country" />
				<entry key="6" value="NEW country" />
				<entry key="17" value="INDIA" />
				<entry key="81" value="Pakistan" />
				<entry key="10" value="USA" />
			</map>
		</property>
		<property name="addressProperties">
			<props>
				<prop key="one">INDIA</prop>
				<prop key="one">NEW</prop>
				<prop key="two">Pakistan</prop>
				<prop key="three">USA</prop>
				<prop key="four">SAME</prop>
				<prop key="one">ADD FOR OVERRIDE</prop>
			</props>
		</property>

	</bean>
</beans>